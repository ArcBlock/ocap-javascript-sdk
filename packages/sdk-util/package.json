{
  "name": "@arcblock/sdk-util",
  "description": "Helpers and utilities shared across ArcBlock Javascript SDK",
  "version": "0.3.0",
  "author": "wangshijun <shijun@arcblock.io> (https://ocap.arcblock.io)",
  "bugs": {
    "url": "https://github.com/ArcBlock/arc-javascript-sdk/issues",
    "email": "shijun@arcblock.io"
  },
  "publishConfig": {
    "access": "public"
  },
  "contributors": [
    "wangshijun <shijun@arcblock.io> (https://ocap.arcblock.io)"
  ],
  "dependencies": {
    "axios": "^0.18.0",
    "debug": "^3.1.0",
    "graphql": "^0.13.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.3.0",
    "eslint-config-prettier": "latest",
    "eslint-plugin-prettier": "latest",
    "jest": "^23.5.0",
    "prettier": "^1.6.1"
  },
  "homepage": "https://github.com/ArcBlock/arc-javascript-sdk",
  "keywords": [
    "ocap",
    "blockchain",
    "arcblock",
    "sdk",
    "nodejs"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "browser": "./lib/index.js",
  "files": [
    "dist",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ArcBlock/arc-javascript-sdk"
  },
  "scripts": {
    "init": "yarn",
    "lint": "eslint src",
    "precommit": "CI=1 npm test",
    "prepush": "CI=1 npm test",
    "test": "npm run lint && node tools/jest.js",
    "coverage": "npm run test -- --coverage",
    "build-browser": "BABEL_ENV=browser babel ./src --out-dir ./lib --source-maps --copy-files",
    "build-node": "babel ./src --out-dir ./dist --source-maps --copy-files",
    "build": "npm run build-node && npm run build-browser"
  }
}
